{
  "name": "gregoriohc/laravel-castable",
  "description": "Laravel package that adds custom casts for models attributes",
  "keywords": ["laravel", "package", "properties", "attributes", "model", "cast"],
  "homepage": "https://github.com/gregoriohc/laravel-castable",
  "license": "MIT",
  "authors": [
    {
      "name": "Gregorio HernÃ¡ndez Caso",
      "email": "gregoriohc@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "5.5.*|5.6.*",
    "illuminate/database": "5.5.*|5.6.*",
    "spinen/laravel-geometry": "^0.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "mockery/mockery": "~1.0",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Gregoriohc\\Castable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gregoriohc\\Castable\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gregoriohc\\Castable\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
