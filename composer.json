{
  "name": "gregoriohc/laravel-castable",
  "type": "library",
  "description": "Laravel package that adds custom casts for models attributes",
  "keywords": [
    "gregoriohc",
    "castable",
    "laravel",
    "properties",
    "attributes",
    "model",
    "cast"
  ],
  "homepage": "https://github.com/gregoriohc/laravel-castable",
  "license": "MIT",
  "authors": [
    {
      "name": "Gregorio Hern√°ndez Caso",
      "email": "gregoriohc@gmail.com",
      "homepage": "https://github.com/gregoriohc",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : "~5.6|~7.0",
    "illuminate/support": "^5.0,<5.6",
    "illuminate/database": "^5.0,<5.6",
    "spinen/laravel-geometry": "^0.6.0"
  },
  "require-dev": {
    "phpunit/phpunit" : ">=4.0",
    "squizlabs/php_codesniffer": "^2.3",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Gregoriohc\\Castable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gregoriohc\\Castable\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gregoriohc\\Castable\\ServiceProvider"
      ],
      "aliases": {
        "Skeleton": "Gregoriohc\\Castable\\Facade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
